{
  "name": "race-application-client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --config vite.config.dev.ts --mode development",
    "build": "tsc -b && vite build",
    "build:debug": "tsc -b && vite build --mode development",
